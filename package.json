  "name": "incubed-example",
  "scripts": {
    "build": "npm run truffle-compile && webpack-cli --config webpack.config.js --mode=production",
    "build-dev": "npm run truffle-compile && webpack-cli --config webpack.config.js --mode=development",
    "build-dev-watch": "npm run truffle-compile && webpack-cli --config webpack.config.js --mode=development --watch",
    "start": "electron -r source-map-support/register . --dev",
    "truffle-compile": "tsc && truffle compile --network development",
    "ts-compile": "tsc",
    "compile": "npm run truffle-compile && npm run ts-compile",
    "test": "truffle test lib/test/node-watch.spec.js lib/test/block-watch.spec.js lib/test/node-registry-api.spec.js",
    "migrate-development": "truffle migrate --network development",
    "ganache": "ganache-cli --account=\"0x4d5db4107d237df6a3d58ee5f70ae63d73d7658d4026f2eefd2f204c81682cb7,4000000000000000000000000000000000\" --gasPrice=0x0 --gasLimit=0x8A1200",
